{
  "short_name": "instce",
  "name": "instce",
  "description": "View your Jira tickets, Github pull requests, and other developer needs in the new tab page.",
  "version": "0.1.1",
  "manifest_version": 2,
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["./static/js/index.js"]
  },
  "permissions": [
    "activeTab",
    "identity",
    "geolocation"
  ]
}
